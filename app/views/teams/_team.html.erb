<% @teams.each do |t| %>
  <div class="col-lg-3 col-md-6">
      <div class="panel panel-primary">
          <div class="panel-heading">
              <div class="row">
                  <div class="col-xs-3">
                      <i class="fa fa-dribbble fa-5x"></i>
                  </div>
                  <div class="col-xs-9 text-right">
                    <div class="huge"><%= t.announces.count %></div>

                  </div>
              </div>
              <div class="row"><h4 class="text-center"><%= t.name %></h4></div>
          </div>
          <%= link_to team_path(t) do %>
              <div class="panel-footer">
                  <span class="pull-left">View Details</span>
                  <span class="pull-right"><i class="fa fa-dribbble"></i></span>
                  <div class="clearfix"></div>
              </div>
          <% end %>
      </div>
  </div>
<% end %>
<%= link_to new_team_path do %>
  <div class="col-lg-3 col-md-6">
      <div class="panel panel-primary">
          <div class="panel-heading">
              <div class="row">
                  <div class="col-xs-3">
                      <i class="fa fa-dribbble fa-5x"></i>
                  </div>
                  <div class="col-xs-9 text-right">
                  </div>
              </div>
              <div class="row"><h4 class="text-center">New Team</h4></div>
          </div>
          <div class="panel-footer">
              <span class="pull-left"></span>
              <span class="pull-right"><i class="fa fa-dribbble"></i></span>
              <div class="clearfix"></div>
          </div>
      </div>
  </div>
<% end %>
